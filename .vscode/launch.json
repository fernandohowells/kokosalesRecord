{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run application",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
{
  "database_name": "sales_records",
  "file_path": "/sales_records/",
  "fields": [
    {
      "field_name": "product_name",
      "data_type": "string"
    },
    {
      "field_name": "unit_price",
      "data_type": "float"
    },
    {
      "field_name": "quantity",
      "data_type": "integer"
    },
    {
      "field_name": "total_price",
      "data_type": "float"
    },
    {
      "field_name": "date",
      "data_type": "date"
    },
    {
      "field_name": "time",
      "data_type": "time"
    }
  ]
}
